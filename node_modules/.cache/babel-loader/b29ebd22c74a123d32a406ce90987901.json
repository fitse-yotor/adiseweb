FA;AAMA,kBAAA,QAAA,EAAA,kBAAA,OAAA,EAAA;AACA,wBAAA,OAAA,EAAA;AACA;AACA,sBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,sBAAA,KAAA,CAAA,aAAA,CAAA,EAAA;AACA;AACA;AAZA,iBADA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA;AAlCA,IATA;AA6CA,EAAA,IA7CA,kBA6CA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,MADA;AAEA,MAAA,MAAA,EAAA,KAFA;AAGA,MAAA,QAAA,EAAA,EAHA;AAIA,MAAA,OAAA,EAAA;AACA,QAAA,IAAA,EAAA,EADA;AAEA,QAAA,IAAA,EAAA,EAFA;AAGA,QAAA,WAAA,EAAA,EAHA;AAIA,QAAA,SAAA,EAAA,KAJA;AAKA,QAAA,aAAA,EAAA;AALA;AAJA,KAAA;AAYA;AA1DA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- Dialog start -->\n      <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n        <template v-slot:activator=\"{ on, attrs }\">\n          <v-btn outlined small color=\"primary\" dark v-bind=\"attrs\" v-on=\"on\">\n            Add producy\n          </v-btn>\n        </template>\n        <v-card >\n          <v-card-title>\n            <span class=\"text-h5\">Add product</span>\n          </v-card-title>\n          <v-card-text>\n            <!-- something -->\n            <v-container>\n              <v-row>\n                <v-col cols=\"12\" >\n                  <v-text-field\n                    label=\"Product name*\"\n                    required\n                    v-model=\"product.name\"\n                  ></v-text-field>\n                </v-col>\n                \n                <v-col cols=\"12\">\n                  <v-text-field v-model=\"product.description\" label=\"Description\" required></v-text-field>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-text-field v-model=\"product.code\" label=\"Product Code\" required></v-text-field>\n                </v-col>\n\n                <v-col cols=\"12\">\n                  <v-text-field type=\"number\" v-model=\"product.price\" label=\"Product Price in birr\" required></v-text-field>\n                </v-col>\n                \n              </v-row>\n            </v-container>\n            <small>*indicates required field</small>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\n              Close\n            </v-btn>\n            <v-btn color=\"blue darken-1\" text @click=\"localaddproduct()\">\n              Save\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    <!-- Dialog end -->\n\n    <!-- Table -->\n    <v-data-table\n      :headers=\"headers\"\n      :items=\"products\"\n      :items-per-page=\"5\"\n      class=\"elevation-1\"\n    >\n      <template v-slot:item.available=\"{ item }\">\n        <v-chip :color=\"getColor(item.available)\" dark small>\n          <b> {{ item.available }} </b>\n        </v-chip>\n      </template>\n\n      <template v-slot:item.actions=\"{ item }\">\n        <v-row>\n          <v-btn text x-small color=\"green\">\n            <v-icon color=\"green\" @click=\"toDetails(item.id)\" small>mdi-details</v-icon>\n          </v-btn>\n\n          <v-btn text x-small color=\"red\">\n            <v-icon small color=\"red\" @click=\"localDeleteProduct(item.id)\">mdi-delete</v-icon>\n          </v-btn>\n        </v-row>\n      </template>\n    </v-data-table>\n    <!-- End table -->\n\n  </div>\n</template>\n\n\n<script>\nimport header from \"@/constants/headers/product.header.js\";\nimport { mapActions, mapState } from \"vuex\";\n\nexport default {\n  name: \"productsList\",\n\n  computed: {\n    ...mapState(\"products\", [\"products\"]),\n  },\n  mounted() {\n    this.getProducts();\n  },\n  methods: {\n    ...mapActions(\"products\", [\"getProducts\",\"addProduct\",\"deleteProduct\"]),\n\n    \n    toDetails(id){\n      this.$router.push('/products/' + id.toString());\n    },\n\n    getColor(value) {\n      return value ? \"green\" : \"red\";\n    },\n\n    localaddproduct(){\n      this.addProduct(this.product)\n    },\n\n    async localDeleteProduct(id){\n       this.$confirm(\n        {\n          message: `Are you sure you want to delete this product?`,\n          button: {\n            no: 'No',\n            yes: 'Yes'\n          },\n          callback: confirm => {\n            if (confirm) {\n              // ... do something\n              console.log(\"Yes\")\n              this.deleteProduct(id)\n            }\n          }\n        }\n      )\n\n    }\n  },\n  data() {\n    return {\n      headers: header,\n      dialog: false,\n      desserts: [],\n      product: {\n        name: \"\",\n        code: \"\",\n        description: \"\",\n        available: false,\n        viewprevprice: false\n      }\n    };\n  },\n};\n</script>"],"sourceRoot":"src/views/dashboard/products"}]}
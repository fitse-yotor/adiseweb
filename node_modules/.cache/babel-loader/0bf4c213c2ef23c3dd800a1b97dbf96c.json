,UAACC,GAAD,EAAS;AACrD1B,kBAAAA,IAAI,CAAC0B,GAAG,CAACI,IAAL,CAAJ;AACH,iBAFK,CAHwB;;AAAA;AAAA;AAAA,uBAMxB/B,cAAc,CAACgC,iBAAf,CAAiCD,IAAI,CAACE,EAAtC,EAA0CP,IAA1C,CAAgD,UAACC,GAAD,EAAS;AACnDF,kBAAAA,OAAO,CAACK,MAAR,CAAe,YAAf,EAA6BH,GAAG,CAACI,IAAjC;AACH,iBAFH,CANwB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASjC,KAnCI;AAqCCO,IAAAA,aArCD,yBAqCeb,OArCf,EAqCwBQ,EArCxB,EAqC2B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACtBjC,cAAc,CAACsC,aAAf,CAA6BL,EAA7B,EAAiCP,IAAjC,CAAuC,UAACC,GAAD,EAAS;AAClDA,kBAAAA,GAAG;AACH1B,kBAAAA,IAAI,gDAAyCgC,EAAzC,EAAJ;AACH,iBAHK,CADsB;;AAAA;AAAA;AAAA,uBAKtBjC,cAAc,CAACuB,WAAf,GAA6BG,IAA7B,CAAmC,UAACC,GAAD,EAAS;AAC9CF,kBAAAA,OAAO,CAACK,MAAR,CAAe,aAAf,EAA8BH,GAAG,CAACI,IAAlC;AACH,iBAFK,CALsB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQ/B;AA7CI;AAtCE,CAAf","sourcesContent":["import productService from \"../services/product.service\"\nimport { info } from '../services/logger.service'\n\nexport default {\n    namespaced: true,\n    state: {\n        products: [],\n        product: {}\n    },\n\n    mutations: {\n        setProducts(state, payload){\n            state.products = payload\n        },\n        setProduct(state, payload){\n            state.product = payload\n        },\n        setProductCategory(state, payload){\n            state.product.category = payload\n        },\n        setProductSupplier(state, payload){\n            state.product.supplier = payload\n        },\n        setProductPrice(state, payload){\n            state.product.price = payload\n        },\n        setProductDescription(state, payload){\n            state.product.description = payload\n        },\n        setProductName(state, payload){\n            state.product.name = payload\n        },\n        setProductCode(state, payload){\n            state.product.code = payload\n        },\n    },\n\n    getters: {\n        getProducts: (state) => {return state.products }\n    },\n\n    actions: {\n        \n        async getProducts(context){\n            await productService.getProducts().then( (res) => {\n                console.log(res)\n                        context.commit('setProducts', res.data)\n                    })\n        },\n\n        async getProductDetails(context, id){\n            await productService.getProductDetails(id).then( (res) => {\n                console.log(res)\n                        context.commit('setProduct', res.data)\n                    })\n        },\n\n        async addProduct(context, data){\n            \n            await productService.addProducts(data).then( (res) => {\n                info(res.data)\n            })\n            await productService.getProducts().then( (res) => {\n                context.commit('setProducts', res.data)\n            })\n        },\n\n        async updateProduct(context, data){\n            data.category = data.category.id;\n            data.supplier = data.supplier.id;\n            await productService.updateProducts(data).then( (res) => {\n                info(res.data)\n            })\n            await productService.getProductDetails(data.id).then( (res) => {\n                        context.commit('setProduct', res.data)\n                    })\n        },\n\n        async deleteProduct(context, id){\n            await productService.deleteProduct(id).then( (res) => {\n                res\n                info(`Successfully deleted product with id ${id}`)\n            })\n            await productService.getProducts().then( (res) => {\n                context.commit('setProducts', res.data)\n            })\n        }\n    }\n}"]}]}
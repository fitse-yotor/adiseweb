plate>\n  <div>\n    <!-- Dialog start -->\n    <v-row justify=\"center\">\n      <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n        <template v-slot:activator=\"{ on, attrs }\">\n          <v-btn color=\"primary\" dark v-bind=\"attrs\" v-on=\"on\">\n            Add producy\n          </v-btn>\n        </template>\n        <v-card color=\"white\">\n          <v-card-title>\n            <span class=\"text-h5\">Add product</span>\n          </v-card-title>\n          <v-card-text>\n            <v-container>\n              <v-row>\n                <v-col cols=\"12\" >\n                  <v-text-field\n                    label=\"Product name*\"\n                    required\n                    v-model=\"product.name\"\n                  ></v-text-field>\n                </v-col>\n                \n                <v-col cols=\"12\">\n                  <v-text-field v-model=\"product.description\" label=\"Description\" required></v-text-field>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-text-field v-model=\"product.code\" label=\"Product Code\" required></v-text-field>\n                </v-col>\n                \n              </v-row>\n            </v-container>\n            <small>*indicates required field</small>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\n              Close\n            </v-btn>\n            <v-btn color=\"blue darken-1\" text @click=\"localaddproduct()\">\n              Save\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-row>\n    <!-- Dialog end -->\n\n\n    <!-- Table -->\n    <v-data-table\n      :headers=\"headers\"\n      :items=\"products\"\n      :items-per-page=\"5\"\n      class=\"elevation-1\"\n    >\n      <template v-slot:item.available=\"{ item }\">\n        <v-chip :color=\"getColor(item.available)\" dark small>\n          <b> {{ item.available }} </b>\n        </v-chip>\n      </template>\n\n      <template v-slot:item.actions=\"{}\">\n        <v-row>\n          <v-btn text x-small color=\"green\">\n            <v-icon color=\"green\" small>mdi-details</v-icon>\n          </v-btn>\n\n          <v-btn text x-small color=\"red\">\n            <v-icon small color=\"red\">mdi-delete</v-icon>\n          </v-btn>\n        </v-row>\n      </template>\n    </v-data-table>\n    <!-- End table -->\n\n  </div>\n</template>\n\n\n<script>\nimport header from \"@/constants/headers/product.header.js\";\nimport { mapActions, mapState } from \"vuex\";\n\nexport default {\n  name: \"productsList\",\n\n  computed: {\n    ...mapState(\"products\", [\"products\"]),\n  },\n  mounted() {\n    this.getProducts();\n  },\n  methods: {\n    ...mapActions(\"products\", [\"getProducts\",\"addProduct\"]),\n\n    getColor(value) {\n      return value ? \"green\" : \"red\";\n    },\n\n    localaddproduct(){\n      this.addProduct(this.product)\n    }\n  },\n  data() {\n    return {\n      headers: header,\n      dialog: false,\n      desserts: [],\n      product: {\n        name: \"\",\n        code: \"\",\n        description: \"\",\n        available: false,\n        viewprevprice: false\n      }\n    };\n  },\n};\n</script>"],"sourceRoot":"src/views/dashboard/products"}]}
AA,KAAA,CAAA,cAAA,CAAA,EAAA;AACA;AACA;AAZA,iBADA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA;AA7BA,IATA;AAwCA,EAAA,IAxCA,kBAwCA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,MADA;AAEA,MAAA,MAAA,EAAA,KAFA;AAGA,MAAA,QAAA,EAAA,EAHA;AAIA,MAAA,QAAA,EAAA;AACA,QAAA,IAAA,EAAA,EADA;AAEA,QAAA,WAAA,EAAA,EAFA;AAGA,QAAA,MAAA,EAAA;AAHA;AAJA,KAAA;AAUA;AAnDA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- Dialog start -->\n      <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n        <template v-slot:activator=\"{ on, attrs }\">\n          <v-btn outlined small color=\"primary\" dark v-bind=\"attrs\" v-on=\"on\">\n            Add Category\n          </v-btn>\n        </template>\n        <v-card >\n          <v-card-title>\n            <span class=\"text-h5\">Add Category</span>\n          </v-card-title>\n          <v-card-text>\n            <!-- something -->\n            <v-container>\n              <v-row>\n                <v-col cols=\"12\" >\n                  <v-text-field\n                    label=\"Category name*\"\n                    required\n                    v-model=\"category.name\"\n                  ></v-text-field>\n                </v-col>\n                \n                <v-col cols=\"12\">\n                  <v-text-field v-model=\"category.description\" label=\"Description\" required></v-text-field>\n                </v-col>\n                \n              </v-row>\n            </v-container>\n            <small>*indicates required field</small>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\n              Close\n            </v-btn>\n            <v-btn color=\"blue darken-1\" text @click=\"localaddcategory()\">\n              Save\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    <!-- Dialog end -->\n\n    <!-- Table -->\n    <v-container>\n      <v-data-table\n      :headers=\"headers\"\n      :items=\"categories\"\n      :items-per-page=\"5\"\n      class=\"elevation-1\"\n    >\n      <template v-slot:item.active=\"{ item }\">\n        <v-chip :color=\"getColor(item.active)\" dark small>\n          <b> {{ item.active }} </b>\n        </v-chip>\n      </template>\n\n      <template v-slot:item.actions=\"{ item }\">\n        <v-row>\n          <v-btn text x-small color=\"green\">\n            <v-icon color=\"green\" small>mdi-details</v-icon>\n          </v-btn>\n\n          <v-btn text x-small color=\"red\">\n            <v-icon small color=\"red\" @click=\"localDeleteProduct(item.id)\">mdi-delete</v-icon>\n          </v-btn>\n        </v-row>\n      </template>\n    </v-data-table>\n    </v-container>\n    <!-- End table -->\n\n  </div>\n</template>\n\n\n<script>\nimport header from \"@/constants/headers/category.header.js\";\nimport { mapActions, mapState } from \"vuex\";\n\nexport default {\n  name: \"categoriesList\",\n\n  computed: {\n    ...mapState(\"categories\", [\"categories\"]),\n  },\n  mounted() {\n    this.getCategory();\n  },\n  methods: {\n    ...mapActions(\"categories\", [\"getCategory\",\"addCategory\",\"deleteCategory\"]),\n\n    getColor(value) {\n      return value ? \"green\" : \"red\";\n    },\n\n    localaddcategory(){\n      this.addCategory(this.category)\n    },\n\n    async localDeleteProduct(id){\n       this.$confirm(\n        {\n          message: `Are you sure you want to delete this product?`,\n          button: {\n            no: 'No',\n            yes: 'Yes'\n          },\n          callback: confirm => {\n            if (confirm) {\n              // ... do something\n              console.log(\"Yes\")\n              this.deleteCategory(id)\n            }\n          }\n        }\n      )\n\n    }\n  },\n  data() {\n    return {\n      headers: header,\n      dialog: false,\n      desserts: [],\n      category: {\n        name: \"\",\n        description: \"\",\n        active: false,\n      }\n    };\n  },\n};\n</script>"],"sourceRoot":"src/views/dashboard/category"}]}
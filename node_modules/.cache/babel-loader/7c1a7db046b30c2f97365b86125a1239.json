;AAEA,kBAAA,MAAA,EAAA;AACA,oBAAA,EAAA,EAAA,IADA;AAEA,oBAAA,GAAA,EAAA;AAFA,mBAFA;AAMA,kBAAA,QAAA,EAAA,kBAAA,OAAA,EAAA;AACA,wBAAA,OAAA,EAAA;AACA;AACA,sBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,sBAAA,KAAA,CAAA,cAAA,CAAA,EAAA;AACA;AACA;AAZA,iBAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA;AA9BA,IATA;AAyCA,EAAA,IAzCA,kBAyCA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,MADA;AAEA,MAAA,MAAA,EAAA,KAFA;AAGA,MAAA,QAAA,EAAA,EAHA;AAIA,MAAA,QAAA,EAAA;AAJA,KAAA;AAMA;AAhDA,CAAA","sourcesContent":["<template>\n  <div>\n    <!-- Dialog start -->\n    <v-dialog v-model=\"dialog\" persistent max-width=\"50%\">\n      <template v-slot:activator=\"{ on, attrs }\">\n        <v-btn outlined small color=\"primary\" dark v-bind=\"attrs\" v-on=\"on\">\n          Add Supplier\n        </v-btn>\n      </template>\n      <v-card>\n        <v-card-title>\n          <span class=\"text-h5\">Add Supplier</span>\n        </v-card-title>\n        <v-card-text>\n          <!-- something -->\n          <v-container>\n            <v-row>\n              <v-text-field\n                style=\"margin-right: 100px\"\n                label=\"Supplier First Name*\"\n                required\n                v-model=\"supplier.fname\"\n              ></v-text-field>\n\n              <v-text-field\n                v-model=\"supplier.lname\"\n                label=\"Supplier Last Name\"\n                required\n              ></v-text-field>\n\n              <v-col cols=\"12\">\n                <v-text-field\n                  v-model=\"supplier.code\"\n                  label=\"Supplier Code\"\n                  required\n                ></v-text-field>\n              </v-col>\n\n              <v-col cols=\"12\">\n                <v-text-field\n                  v-model=\"supplier.email\"\n                  label=\"Email\"\n                  required\n                ></v-text-field>\n              </v-col>\n\n              <v-col cols=\"12\">\n                <v-text-field\n                  v-model=\"supplier.address\"\n                  label=\"Address\"\n                  required\n                ></v-text-field>\n              </v-col>\n\n              <v-text-field\n                style=\"margin-right: 100px\"\n                v-model=\"supplier.country\"\n                label=\"Country\"\n                required\n              ></v-text-field>\n\n              <v-text-field\n                v-model=\"supplier.city\"\n                label=\"City\"\n                required\n              ></v-text-field>\n\n              <v-col cols=\"12\">\n                <v-text-field\n                  v-model=\"supplier.phonenum\"\n                  label=\"Phone number\"\n                  required\n                ></v-text-field>\n              </v-col>\n            </v-row>\n          </v-container>\n          <small>*indicates required field</small>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\n            Close\n          </v-btn>\n          <v-btn color=\"blue darken-1\" text @click=\"localaddsupplier()\">\n            Save\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n    <!-- Dialog end -->\n\n    <!-- Table -->\n    <v-main>\n      <v-container>\n        <v-data-table\n      :headers=\"headers\"\n      :items=\"suppliers\"\n      :items-per-page=\"5\"\n      class=\"elevation-1\"\n    >\n      <template v-slot:item.available=\"{ item }\">\n        <v-chip :color=\"getColor(item.available)\" dark small>\n          <b> {{ item.available }} </b>\n        </v-chip>\n      </template>\n\n      <template v-slot:item.actions=\"{ item }\">\n        <v-row>\n          <v-btn text x-small color=\"green\">\n            <v-icon color=\"green\" small>mdi-details</v-icon>\n          </v-btn>\n\n          <v-btn text x-small color=\"red\">\n            <v-icon small color=\"red\" @click=\"localDeleteProduct(item.id)\"\n              >mdi-delete</v-icon\n            >\n          </v-btn>\n        </v-row>\n      </template>\n    </v-data-table>\n      </v-container>\n    </v-main>\n    <!-- End table -->\n  </div>\n</template>\n\n\n<script>\nimport header from \"@/constants/headers/supplier.header.js\";\nimport { mapActions, mapState } from \"vuex\";\n\nexport default {\n  name: \"productsList\",\n\n  computed: {\n    ...mapState(\"suppliers\", [\"suppliers\"]),\n  },\n  mounted() {\n    this.getSuppliers();\n  },\n  methods: {\n    ...mapActions(\"suppliers\", [\n      \"getSuppliers\",\n      \"addSuppliers\",\n      \"deleteSupplier\",\n    ]),\n\n    getColor(value) {\n      return value ? \"green\" : \"red\";\n    },\n\n    localaddsupplier() {\n      this.addSuppliers(this.supplier);\n    },\n\n    async localDeleteProduct(id) {\n      this.$confirm({\n        message: `Are you sure you want to delete this product?`,\n        button: {\n          no: \"No\",\n          yes: \"Yes\",\n        },\n        callback: (confirm) => {\n          if (confirm) {\n            // ... do something\n            console.log(\"Yes\");\n            this.deleteSupplier(id);\n          }\n        },\n      });\n    },\n  },\n  data() {\n    return {\n      headers: header,\n      dialog: false,\n      desserts: [],\n      supplier: {},\n    };\n  },\n};\n</script>"],"sourceRoot":"src/views/dashboard/suppliers"}]}
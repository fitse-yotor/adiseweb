d\n            ></v-text-field>\n          </v-col>\n          \n          <v-col cols=\"10\">\n            <v-text-field\n              v-model=\"code\"\n              label=\"Product Code\"\n              required\n              dense\n              outlined\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"10\">\n            <v-text-field\n              type=\"number\"\n              v-model=\"price\"\n              label=\"Price\"\n              required\n              dense\n              outlined\n            ></v-text-field>\n          </v-col>\n        </v-col>\n\n        <v-spacer />\n         <v-col > \n        <div class=\"imagecontainer\"></div>\n        <v-btn\n        class=\"ma-2\"\n        dark\n      >\n        <v-icon\n          dark\n          left\n        >\n          mdi-camera\n        </v-icon> Add Image\n      </v-btn>\n         </v-col>\n        <v-spacer />\n      </v-row>\n\n      <!-- <v-col cols=\"4\"> -->\n      <v-btn @click=\"localUpdate()\" class=\"primary\" style=\"width: 50%; margin-top: 5%\"> Update </v-btn>\n      <!-- </v-col> -->\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\n\nexport default {\n  computed: {\n    ...mapState(\"products\", [\"product\"]),\n    ...mapState(\"suppliers\", [\"suppliers\"]),\n    ...mapState(\"categories\", [\"categories\"]),\n\n    price: {\n        get () {\n          return this.$store.state.products.product.price\n        },\n        set (value) {\n          this.$store.commit('products/setProductPrice', value)\n        }\n    },\n\n    name: {\n        get () {\n          return this.$store.state.products.product.name\n        },\n        set (value) {\n          this.$store.commit('products/setProductName', value)\n        }\n    },\n    category: {\n        get () {\n          return this.$store.state.products.product.category\n        },\n        set (value) {\n          this.$store.commit('products/setProductCategory', value)\n        }\n    },\n    description: {\n        get () {\n          return this.$store.state.products.product.description\n        },\n        set (value) {\n          this.$store.commit('products/setProductDescription', value)\n        }\n    },\n    code: {\n        get () {\n          return this.$store.state.products.product.code\n        },\n        set (value) {\n          this.$store.commit('products/setProductCode', value)\n        }\n    },\n    supplier: {\n        get () {\n          return this.$store.state.products.product.supplier\n        },\n        set (value) {\n          console.log(value)\n          this.$store.commit('products/setProductSupplier', value)\n        }\n    }\n  },\n\n  data() {\n    return {\n          };\n  },\n  mounted() {\n    this.getProductDetails(this.$route.params.id);\n    this.getSuppliers();\n    this.getCategory();\n  },\n  methods: {\n    ...mapActions(\"products\", [\"getProductDetails\", \"updateProduct\"]),\n    ...mapActions(\"suppliers\", [\"getSuppliers\"]),\n    ...mapActions(\"categories\", [\"getCategory\"]),\n\n    localUpdate(){\n      //  console.log(this.product)\n      // this.$store.commit('products/setProductSupplier', this.$store.state.products.product.supplier.id);\n      // this.$store.commit('products/setProductCategory', this.$store.state.products.product.category.id);\n      this.updateProduct(this.product)\n    }\n  },\n};\n</script>\n\n\n<style>\n\n.imagecontainer{\n  background-color: red;\n  width: 35vh;\n  height: 35vh;\n  border-radius: 2vh;\n  background: url(\"../../../assets/images/noimage.jpeg\");\n  background-size: 100%;\n  background-repeat: no-repeat;\n  background-attachment: contain;\n  background-position: center;\n  }\n</style>"]}]}
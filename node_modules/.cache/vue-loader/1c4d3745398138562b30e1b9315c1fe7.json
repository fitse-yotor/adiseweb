 @click=\"localDeleteOrder(item.id)\"\n                  >mdi-delete</v-icon\n                >\n              </v-btn>\n            </v-row>\n          </template>\n\n        </v-data-table>\n        <!-- End table -->\n      </v-card-text>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nimport header from \"@/constants/headers/orders.header.js\";\n\n\nexport default {\n\n  data() {\n    return {\n      headers: header,\n    }\n  },\n\n  computed: {\n        ...mapState(\"order\", [\"orders\"]),\n  },\n\n  mounted() {\n    this.getOrders()\n  },\n\n  methods: {\n        ...mapActions(\"order\", [\"deleteOrder\", \"getOrders\", \"addOrder\"]),\n\n        async localDeleteOrder(id) {\n          this.$confirm({\n            message: `Are you sure you want to delete this product?`,\n            button: {\n              no: \"No\",\n              yes: \"Yes\",\n            },\n            callback: (confirm) => {\n              if (confirm) {\n                // ... do something\n                console.log(\"Yes\");\n                this.deleteOrder(id);\n              }\n            },\n          });\n        },\n\n\n    toDetails(id) {\n      this.$router.push(\"/orders/\" + id.toString());\n    },\n\n\n  }\n\n}\n</script>\n\n<style>\n\n</style>"]}]}
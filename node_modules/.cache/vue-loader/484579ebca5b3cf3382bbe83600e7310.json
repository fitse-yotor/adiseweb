   :permanent=\"sidebarMenu\"\n            :mini-variant.sync=\"mini\"\n            >\n            <v-list dense color=\"primary\" dark>\n                <v-list-item>\n                    <v-list-item-action>\n                        <v-icon @click.stop=\"sidebarMenu = !sidebarMenu\" class=\"primary\">mdi-chevron-left</v-icon>\n                    </v-list-item-action>\n                    <h3 class=\"font-weight-thin\">Vuetify Dashboard</h3>\n                </v-list-item>\n            </v-list>\n            <v-list-item class=\"px-2\" @click=\"toggleMini = !toggleMini\">\n                <v-list-item-avatar>\n                    <v-icon>mdi-account-outline</v-icon>\n                </v-list-item-avatar>\n                <v-list-item-content class=\"text-truncate\">\n                    Admin panel\n                </v-list-item-content>\n                <v-btn icon small>\n                    <v-icon>mdi-chevron-left</v-icon>\n                </v-btn>\n            </v-list-item>\n            <v-divider></v-divider>\n            <v-list>\n                <v-list-item v-for=\"item in items\" :key=\"item.title\" link :to=\"'/'+item.route\">\n                    <v-list-item-icon>\n                        <v-icon color=\"\">{{ item.icon }}</v-icon>\n                    </v-list-item-icon>\n                    <v-list-item-content>\n                        <v-list-item-title class=\"title_nav\">{{ item.title }}</v-list-item-title>\n                    </v-list-item-content>\n                </v-list-item>\n            </v-list>\n        </v-navigation-drawer>\n        <v-content>\n            <v-container class=\"px-4 py-0 fill-height\" fluid>\n                <v-row class=\"fill-height\">\n                    <v-col>\n                        <transition name=\"fade\">\n                            <router-view></router-view>\n                        </transition>\n                    </v-col>\n                </v-row>\n            </v-container>\n        </v-content>\n        <v-footer class=\"py-3\">\n            <span class=\"ml-auto overline\">Addis Ecom &copy;2020</span>\n        </v-footer>\n    </v-app>\n\n</div>\n\n\n</template>\n\n<script>\n\nimport navitems from '../constants/navbar'\n  export default {\n    components: {\n    },\n    computed: {\n      items(){\n        return navitems\n      },\n    mini() {\n        return (this.$vuetify.breakpoint.smAndDown) || this.toggleMini\n    },\n    buttonText() {\n        return !this.$vuetify.theme.dark ? 'Go Dark' : 'Go Light'\n    }\n  },\n    data () {\n      return {\n      sidebarMenu: true,\n      toggleMini: true,\n      \n      }\n    },\n\n    methods: {\n    toggleTheme() {\n        this.$vuetify.theme.dark = !this.$vuetify.theme.dark;      \n    },\n  }\n  }\n</script>\n\n<style scoped>\n  .title_nav {\n    text-align: start;\n  }\n</style>"]}]}
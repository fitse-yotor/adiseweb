ol>\n\n        <v-col cols=\"6\">\n          <v-text-field\n            label=\"Phone Number\"\n            v-model=\"order_details.customer_phonenum\"\n            required\n            dense\n            outlined\n            disabled\n          ></v-text-field>\n        </v-col>\n\n        <v-col cols=\"6\">\n          <v-text-field\n            label=\"Customer address\"\n            v-model=\"order_details.customer_address\"\n            required\n            dense\n            outlined\n            disabled\n          ></v-text-field>\n        </v-col>\n\n        <v-col cols=\"6\">\n          <v-text-field\n            label=\"Customer City\"\n            v-model=\"order_details.customer_city\"\n            required\n            dense\n            outlined\n            disabled\n          ></v-text-field>\n        </v-col>\n\n        <v-col cols=\"6\">\n          <v-text-field\n            label=\"Discount Code\"\n            v-model=\"order_details.discount_code\"\n            required\n            dense\n            outlined\n            disabled\n          ></v-text-field>\n        </v-col>\n\n        <v-col cols=\"6\">\n          <v-text-field\n            label=\"Total Cost\"\n            v-model=\"order_details.total_cost\"\n            required\n            dense\n            outlined\n            disabled\n          ></v-text-field>\n        </v-col>\n\n        <!-- {{ order_details.products }} -->\n      </v-row>\n\n\n      <div class=\"products\">\n        <v-card-title> Products </v-card-title>\n\n        <v-row>\n          <v-card\n            v-for=\"product in order_details.products\"\n            :key=\"product.id\"\n            class=\"mr-10 ml-10 my-12\"\n            max-width=\"250\"\n          >\n            <template slot=\"progress\">\n              <v-progress-linear\n                color=\"deep-purple\"\n                height=\"10\"\n                indeterminate\n              ></v-progress-linear>\n            </template>\n\n            <v-img\n              height=\"150\"\n              :src=\"'http://localhost:3000/' + product.images\"\n            ></v-img>\n\n            <v-card-subtitle> \n              {{ product.name }}   <br />\n              Price:  {{ product.price }}$   <br />\n             Quantity:  {{ product.quantity }}\n            </v-card-subtitle>\n           \n            <v-divider class=\"mx-4\"></v-divider>\n\n            <v-card-actions>\n              <v-btn color=\"deep-purple lighten-2\" text> View Details </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-row>\n      </div>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\n\nexport default {\n  computed: {\n    ...mapState(\"order\", [\"order_details\"]),\n  },\n  mounted() {\n    this.initDetails();\n  },\n  methods: {\n    ...mapActions(\"order\", [\"getOrderDetails\"]),\n\n    async initDetails() {\n      await this.getOrderDetails(this.$route.params.id);\n      var products = [];\n      var temp_products = JSON.parse(\n        this.$store.state.order.order_details.products\n      );\n      for (var i in temp_products) {\n        var produc_details = await this.$store.dispatch(\n          \"products/getProductDetails\",\n          temp_products[i].id\n        );\n        if (produc_details.images != \"\") {\n          produc_details.images = produc_details.images\n            .toString()\n            .split(\";\")[0];\n        }\n        produc_details.quantity = temp_products[i].quantity;\n        products.push(produc_details);\n      }\n      this.$store.commit(\"order/updateProductsInOrder\", products);\n    },\n  },\n};\n</script>\n\n<style>\n</style>"]}]}